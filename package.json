    "test:unit": "vitest",
    "format": "prettier --write src/",
    "storybook": "cross-env STORYBOOKBUILD=true storybook dev -p 6006",
    "build-storybook": "cross-env STORYBOOKBUILD=true storybook build",
    "patch": "yarn version --patch"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "^5.0.17",
    "@mdi/font": "^7.4.47",
    "pinia": "^2.2.2",
    "vue": "^3.5.2",
    "vue-router": "^4.4.3",
    "vuetify": "^3.7.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-viewport": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "storybook": "^8.2.9",
    "storybook-addon-vue-mdx": "^1.0.1",
    "storybook-vue-addon": "^0.6.1",
    "vite": "^5.4.3",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "@swc/core": "1.6.0"
    }
  }
